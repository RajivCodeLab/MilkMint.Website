{
  "version": 2,
  "compress": true,
  "images": {
    "domains": ["*"],
    "sizes": [320, 420, 768, 1024, 1200],
    "minimumCacheTTL": 60
  },
  "functions": {
    "api/**": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/(.*)", "headers": { "Cache-Control": "public, max-age=31536000, immutable" }, "continue": true }
  ]
}
